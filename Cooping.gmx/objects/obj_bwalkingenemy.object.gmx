//walkspeed = speed of the enemy

hspd = dir*walkspeed;


//Gravity Check to ensure they do not go through floors

if(place_meeting(x,y+1,obj_solid))
{
    y+=0;
}

//Horitzonal Collisions with a wall
if(place_meeting(x+hspd,y,obj_solid))
{
    if(!place_meeting(x+sign(hspd),y,obj_solid))
    {
        x+=sign(hspd);
    }   
    dir*=-1;
}

//If nothing is
if(place_free(x+sign(hspd)*(sprite_width/2),y+1))
{
    dir*=-1;
}

x+=hspd;



//Enemy Collision
if(place_meeting(x,y,obj_nPlayer))
{
    if (obj_nPlayer.y &lt; y-16)
    {
        with (obj_nPlayer) vsp = -jspd;
        instance_destroy();
    }
    else
    {
        
        room_restart();
    }
}
        
</string>
